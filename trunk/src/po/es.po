msgid ""
"   left      previous tool   \n"
"   right     next tool       \n"
"   ','       previous object \n"
"   '.'       next object     \n"
" +^'a'       deselect all currently selected objects\n"
"   '<'       previous page   \n"
"   '>'       next page       \n"
